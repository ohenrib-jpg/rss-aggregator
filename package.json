    "pg": "^8.11.3",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "nodemon": "^3.0.1",
    "sqlite3": "^5.1.7"
  },
  "optionalDependencies": {
